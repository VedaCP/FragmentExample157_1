<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.fragmentexample157_1.TitleFragment"
        android:label="fragment_title"
        tools:layout="@layout/fragment_title" >
        <action
            android:id="@+id/action_titleFragment_to_logoTriviaFragment"
            app:destination="@id/logoTriviaFragment" />
    </fragment>
    <fragment
        android:id="@+id/loserFragment"
        android:name="com.example.fragmentexample157_1.LoserFragment"
        android:label="fragment_loser"
        tools:layout="@layout/fragment_loser" >
        <action
            android:id="@+id/action_loserFragment_to_logoTriviaFragment"
            app:destination="@id/logoTriviaFragment" />
    </fragment>
    <fragment
        android:id="@+id/logoTriviaFragment"
        android:name="com.example.fragmentexample157_1.LogoTriviaFragment"
        android:label="fragment_logo_trivia"
        tools:layout="@layout/fragment_logo_trivia" >
        <action
            android:id="@+id/action_logoTriviaFragment_to_loserFragment"
            app:destination="@id/loserFragment" />
        <action
            android:id="@+id/action_logoTriviaFragment_to_winnerFragment"
            app:destination="@id/winnerFragment" />
    </fragment>
    <fragment
        android:id="@+id/winnerFragment"
        android:name="com.example.fragmentexample157_1.WinnerFragment"
        android:label="fragment_winner"
        tools:layout="@layout/fragment_winner" >
        <action
            android:id="@+id/action_winnerFragment_to_titleFragment"
            app:destination="@id/titleFragment" />
    </fragment>
</navigation>